{
  "name": "phuria/under-query",
  "description": "Lightweight PHP Query Builder primarily focused on building reusable SQL queries.",
  "homepage": "https://github.com/phuria/under-query",
  "keywords": [
    "query",
    "builder",
    "sql",
    "mysql"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Beniamin Jonatan Å imko",
      "email": "spam@simko.it",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "league/pipeline": "^0.2",
    "pimple/pimple": "^3.0",
    "container-interop/container-interop": "^1.1",
    "doctrine/dbal": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "phpunit/dbunit": "^2.0",
    "symfony/var-dumper": "^3.1"
  },
  "suggest": {
    "phuria/sql-builder-bundle": "To integrate with Symfony."
  },
  "autoload": {
    "psr-4": {
      "Phuria\\UnderQuery\\": "src",
      "Phuria\\UnderQuery\\Tests\\": "tests"
    }
  }
}
